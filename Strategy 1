teamname = "HonlasicOld1"
stratname = "Mimic, betray every 7 rounds, betray twice every 15th round."





def move(myhistory, theirhistory, myscore, theirscore):
    if 'b' or 'c' not in theirhistory:
        return 'c'
    elif theirhistory[-1] == 'b':
        return 'b'
    elif (len(theirhistory)-1)%7==0:
        return 'b'
    elif  (len(theirhistory)-1)%15==0:
        return 'b'
    elif (len(theirhistory)-1)%16==0:
        return 'b'
    else:
        return 'c'
